<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/SETUPTUTORIAL_TEXTVIEW_TITLE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_below="@id/SETUPTUTORIAL_TEXTVIEW_TITLE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SETUPTUTORIAL_TEXTVIEW_MYTAGS" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/SETUPTUTORIAL_TEXTVIEW_MYTAGS"
        android:id="@+id/SETUPTUTORIAL_LINEARLAYOUT_MYBOARD"/>

</RelativeLayout>